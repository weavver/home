{
     "name": "weavver-home-api",
     "version": "0.0.1",
     "description": "",
     "main": "app.js",
     "watch": {
          "start": {
               "patterns": [
                    "app.ts",
                    "src"
               ],
               "extensions": "ts",
               "quiet": true,
               "delay": 1000
          }
     },
     "scripts": {
          "test": "mocha -r ts-node/register/transpile-only \"{,!(node_modules)/**}/*.test.ts\"",
          "debug": "nodemon --exec 'node --inspect-brk=5300 --require ts-node/register app.ts'",
          "deploy": "echo \"WARNING: serverless-typescript 1.1.8 and 1.1.9 are broken on OSX\" && dotenv -e ../.env-aws-dev serverless deploy",
          "start": "dotenv -e ../.env ts-node app.ts",
          "serverless-offline": "dotenv -e ../.env serverless offline start --useSeparateProcesses"
     },
     "author": "Mitchel Constantin",
     "license": "MIT",
